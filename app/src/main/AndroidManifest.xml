<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cse110_project">

    <application
        android:name=".WWRApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".TeamRoutesDetailsActivity"></activity>
        <activity android:name=".ScheduledDetails"></activity>
        <activity android:name=".TeamRoutesActivity" />
        <activity android:name=".MockingActivity" />
        <activity
            android:name=".dialogs.LaunchDataDialog"
            android:theme="@style/splashTheme" />
        <activity android:name=".EntryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:launchMode="standard" />
        <activity android:name=".RoutesActivity" />
        <activity android:name=".WalkActivity" />
        <activity android:name=".RouteDetailsActivity" />
        <activity android:name=".TeamActivity" />
    </application>

</manifest>